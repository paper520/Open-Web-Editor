{
  "name": "open-web-editor",
  "version": "0.2.0",
  "description": "✍️ An Editor Used on the Browser Side.",
  "main": "dist/open-web-editor.min.js",
  "scripts": {
    "start": "npm install && npm run build",
    "build": "cuf --config cuf.config.js init && cuf --config cuf.config.js bundle && cuf --config cuf.config.js babel && cuf --config cuf.config.js uglifyjs && cuf --config cuf.config.js end"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hai2007/Open-Web-Editor.git"
  },
  "keywords": [
    "web",
    "editor"
  ],
  "author": "hai2007",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hai2007/Open-Web-Editor/issues"
  },
  "homepage": "https://github.com/hai2007/Open-Web-Editor#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "cuf": "^1.2.0",
    "rollup": "^1.17.0",
    "uglify-js": "^3.5.6"
  },
  "dependencies": {
    "@hai2007/tool": "^0.4.0"
  }
}
