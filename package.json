{
  "name": "open-web-editor",
  "version": "0.0.0-alpha",
  "description": "✍️ An Editor Used on the Browser Side.",
  "main": "dist/open-web-editor.min.js",
  "scripts": {
    "start": "npm install && npm run build",
    "build": "npm run build:init && npm run build:bundle && npm run build:babel && npm run build:uglifyjs && npm run build:end",
    "build:init": "cuf --config ./cuf.config.js init",
    "build:end": "cuf --config ./cuf.config.js end",
    "build:bundle": "cuf --config ./cuf.config.js bundle",
    "build:babel": "cuf --config ./cuf.config.js babel",
    "build:uglifyjs": "cuf --config ./cuf.config.js uglifyjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hai2007/Open-Web-Editor.git"
  },
  "keywords": [
    "web",
    "editor"
  ],
  "author": "hai2007",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hai2007/Open-Web-Editor/issues"
  },
  "homepage": "https://github.com/hai2007/Open-Web-Editor#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "cuf": "^1.2.0",
    "rollup": "^1.17.0",
    "uglify-js": "^3.5.6"
  }
}
